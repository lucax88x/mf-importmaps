{
	"name": "@mf/ui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"files": [
		"types"
	],
	"exports": {
		".": {
			"types": "./types/exports/index.d.ts"
		},
		"./*": {
			"types": "./types/exports/*.d.ts"
		}
	},
	"scripts": {
		"build": "vite build && rm -rf types && tsgo -p tsconfig.build.json",
		"publish:local": "mkdir -p ../../.local-packages && npm pack --pack-destination ../../.local-packages",
		"typecheck": "tsgo --noEmit",
		"preview": "vite preview",
		"dev": "vite --host"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.2.1",
		"react": "catalog:react",
		"react-dom": "catalog:react",
		"tailwindcss": "^4.2.1"
	},
	"devDependencies": {
		"@types/react": "catalog:react",
		"@types/react-dom": "catalog:react",
		"@typescript/native-preview": "^7.0.0-dev",
		"@vitejs/plugin-react": "^5",
		"vite": "8.0.0-beta.15"
	}
}
