{
  "name": "@mf/components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./button": {
      "types": "./dist/Button.d.ts",
      "default": "./src/Button.tsx"
    },
    "./mf-button": {
      "types": "./dist/MfButton.d.ts",
      "default": "./src/MfButton.ts"
    },
    "./calculate": {
      "types": "./dist/calculate.d.ts",
      "default": "./src/calculate.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev": "vite --host"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4",
    "typescript": "^5",
    "vite": "^6"
  }
}
